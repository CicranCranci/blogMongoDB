<%- include('partials/header') %>

    <h1>Compose</h1>
    <form method="post" action="/compose">
        <div class="form-group">
            <label for="title">Title</label>
            <input type="text" class="form-control" name="composeTitle" value="">
        </div>
        <div class="form-group">
            <label for="post">Post</label>
            <textarea class="form-control" name="composeContent" id="Textarea1" row="7"></textarea>
        </div><br>
        <button type="submit" class="btn btn-primary" name="submit" autocomplete="off"> Publish </button>
    </form>

    <%- include('partials/footer') %>